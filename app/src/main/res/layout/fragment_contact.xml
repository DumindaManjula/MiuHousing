<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ContactFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="58dp"
        android:layout_height="58dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="56dp"
        android:background="@drawable/imageview_border"
        android:src="@drawable/baseline_home_24"
       />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/contact_name"
        android:textColor="@color/black"
        android:layout_toRightOf="@+id/imageView7"
        android:layout_marginTop="56dp"
        android:layout_marginLeft="20dp"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/contact_address"
        android:textColor="@color/black"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="20dp"
        android:layout_toRightOf="@+id/imageView7"
        android:layout_below="@+id/textView10" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="58dp"
        android:layout_height="58dp"
        android:background="@drawable/imageview_border"
        android:src="@drawable/baseline_local_phone_24"
        android:layout_below="@+id/imageView7"
        android:layout_marginStart="24dp"
        android:layout_marginTop="56dp"/>

    <TextView
        android:id="@+id/phoneNumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/contact_phone2"
        android:textColor="@color/black"
        android:layout_below="@id/imageView7"
        android:layout_toRightOf="@id/imageView8"
        android:layout_marginStart="20dp"
        android:layout_marginTop="56dp"/>

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="15dp"
        android:text="@string/contact_ext1"
        android:textColor="@color/black"
       android:layout_below="@+id/phoneNumber"
        android:layout_toRightOf="@id/imageView8"/>

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="58dp"
        android:layout_height="58dp"
      android:layout_marginTop="56dp"
        android:layout_marginStart="24dp"
        android:background="@drawable/imageview_border"
        android:src="@drawable/baseline_mail_outline_24"
      android:layout_below="@id/imageView8" />

    <TextView
        android:id="@+id/housingEmail1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="58dp"
        android:textColor="@color/black"
        android:text="@string/contact_email3"
        android:layout_below="@id/imageView8"
        android:layout_toRightOf="@id/imageView9"/>

    <TextView
        android:id="@+id/housingEmail2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/contact_email2"
        android:textColor="@color/black"
        android:layout_below="@id/housingEmail1"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="20dp"
        android:layout_toRightOf="@id/imageView9"/>
</RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>